{
  "name": "ayecode/flarum-restrictions",
  "version": "0.1.8",
  "description": "Restrict Flarum content based on EDD licenses",
  "type": "flarum-extension",
  "private": true,
  "keywords": [
    "flarum",
    "restrictions",
    "edd",
    "license"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "AyeCode Ltd",
      "email": "contact@ayecode.io",
      "role": "Developer"
    }
  ],
  "require": {
    "flarum/core": "^1.8.0"
  },
  "autoload": {
    "psr-4": {
      "AyeCode\\FlarumRestrictions\\": "src/"
    }
  },
  "extra": {
    "flarum-extension": {
      "title": "AyeCode Restrictions",
      "category": "feature",
      "icon": {
        "name": "fas fa-lock",
        "backgroundColor": "#238636",
        "color": "#fff"
      }
    },
    "flarum-assets": {
      "forum": [
        "js/src/forum/dist/forum.js"
      ]
    }
  }
}